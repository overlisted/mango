<meta charset="utf-8">

<meta name="color-scheme" content="only light">
<meta name="viewport" content="initial-scale=1.0, width=device-width">

<style>
  body, body * {
    margin: 0;
    padding: 0;
    text-decoration: none;
    list-style: none;
    font-family: Inter, Ubuntu, Rubik, sans-serif;
    display: block;
    box-sizing: border-box;
    line-height: 1.5;
  }

  a {
    color: {{config.themeColor}};
  }

  html {
    min-height: 100%;
    background-color: #F8F0F8;
  }

  #nav {
    display: flex;
    justify-content: space-between;
  }

  nav ul {
    display: flex;
  }

  nav li {
    margin-right: 16px;
  }

  h1 {
    font-weight: 500;
    font-size: 30px;
  }

  h2 {
    font-weight: 500;
    font-size: 24px;
  }

  .bg {
    background: #FFF;
  }

  .indent {
    padding: 24px;
  }

  .shadow {
    box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 1px 1px, rgba(0, 0, 0, 0.1) 1px 2px 4px;
  }

